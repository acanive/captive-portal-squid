<!DOCTYPE html>
<html>
<head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>{% block title %}Welcome!{% endblock %}</title>
    {% block stylesheets %}
        <link href="{{ asset('assets/css/bootstrap.css') }}" rel="stylesheet">
        <link href="{{ asset('assets/css/materialdesignicons.min.css') }}" rel="stylesheet">
        <link href="{{ asset('assets/css/style.css') }}" rel="stylesheet">
        <link href="{{ asset('assets/plugins/Bootstrap-Validator/css/bootstrapValidator.css') }}" rel="stylesheet">
    {% endblock %}
    <link rel="icon" type="image/x-icon" href="{{ asset('assets/img/internet.png') }}" />

</head>
<body>
        {% block body %}

        {% endblock %}
        {% block footer %}
            <footer>
                <div class="container">
                    <div class="row text-center" style="display: block">
                        <p>
                            Desarrollado por: Grupo de Desarrollo de Aplicaciones Informaticas <strong>(GDAI)</strong>
                            <img src="{{ asset('assets/img/gdai-LOGO.png') }}">
                        </p>
                        <p>
                            Â© 2017 -
                            {% if "now"|date("Y") != 2017 %}
                                {{ "now"|date("Y") }}
                            {% endif %}
                            &nbsp;Todos los derechos reservados
                        </p>
                    </div>
                </div>
            </footer>
        {% endblock %}

{% block javascripts %}
    <script src="{{ asset('assets/js/jquery.js') }}"></script>
    <script src="{{ asset('assets/js/bootstrap.js') }}"></script>
    <script src="{{ asset('assets/js/app.js') }}"></script>
    <script src="{{ asset('assets/plugins/Bootstrap-Validator/js/bootstrapValidator.js') }}"></script>
    <script src="{{ asset('assets/plugins/Bootstrap-Validator/js/es_ES.js') }}"></script>
{% endblock %}
</body>
</html>