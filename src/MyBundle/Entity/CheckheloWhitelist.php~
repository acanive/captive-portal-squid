<?php

namespace MyBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * CheckheloWhitelist
 *
 * @ORM\Table(name="checkhelo_whitelist", uniqueConstraints={@ORM\UniqueConstraint(name="ID", columns={"ID"}), @ORM\UniqueConstraint(name="Source", columns={"Source"})})
 * @ORM\Entity
 */
class CheckheloWhitelist
{
    /**
     * @var integer
     *
     * @ORM\Column(name="ID", type="bigint", nullable=false)
     */
    private $id;

    /**
     * @var string
     *
     * @ORM\Column(name="Source", type="string", length=512, nullable=false)
     */
    private $source;

    /**
     * @var string
     *
     * @ORM\Column(name="Comment", type="string", length=1024, nullable=true)
     */
    private $comment;

    /**
     * @var integer
     *
     * @ORM\Column(name="Disabled", type="smallint", nullable=false)
     */
    private $disabled = '0';



    /**
     * Set id
     *
     * @param integer $id
     *
     * @return CheckheloWhitelist
     */
    public function setId($id)
    {
        $this->id = $id;

        return $this;
    }

    /**
     * Get id
     *
     * @return integer
     */
    public function getId()
    {
        return $this->id;
    }

    /**
     * Set source
     *
     * @param string $source
     *
     * @return CheckheloWhitelist
     */
    public function setSource($source)
    {
        $this->source = $source;

        return $this;
    }

    /**
     * Get source
     *
     * @return string
     */
    public function getSource()
    {
        return $this->source;
    }

    /**
     * Set comment
     *
     * @param string $comment
     *
     * @return CheckheloWhitelist
     */
    public function setComment($comment)
    {
        $this->comment = $comment;

        return $this;
    }

    /**
     * Get comment
     *
     * @return string
     */
    public function getComment()
    {
        return $this->comment;
    }

    /**
     * Set disabled
     *
     * @param integer $disabled
     *
     * @return CheckheloWhitelist
     */
    public function setDisabled($disabled)
    {
        $this->disabled = $disabled;

        return $this;
    }

    /**
     * Get disabled
     *
     * @return integer
     */
    public function getDisabled()
    {
        return $this->disabled;
    }
}
